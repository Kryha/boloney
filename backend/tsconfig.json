{
  "compilerOptions": {
    "plugins": [
      {
        "transform": "ts-auto-mock/transformer",
        "cacheBetweenTests": false
      }
    ],
    "paths": {
      "jest-ts-auto-mock": ["../../index"]
    },
    "moduleResolution": "node",
    "esModuleInterop": true,
    "noUnusedLocals": true,
    "removeComments": true,
    "target": "ESNext",
    "module": "ESNext",
    "strict": true
  },
  "include": ["src", "nakama"],
  "exclude": ["node_modules", "build"]
}
